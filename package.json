  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-root-import": "6.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
