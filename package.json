{
  "name": "tappytanks-server",
  "version": "0.0.1",
  "description": "server for tappytanks",
  "engines": {
    "node": "11"
  },
  "scripts": {
    "start": "nodemon --inspect src/api.js --exec babel-node",
    "build": "babel src -d dist",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix",
    "test": "jest \"src/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --coverage --forceExit",
    "typed": "flow-typed install",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tankerxyz/tappytanks-server.git"
  },
  "keywords": [
    "Node.js"
  ],
  "author": "Tankerxyz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tankerxyz/tappytanks-server/issues"
  },
  "homepage": "https://github.com/Tankerxyz/tappytanks-server#readme",
  "dependencies": {
    "@tensorflow/tfjs": "0.14.0",
    "@tensorflow/tfjs-node": "0.1.21",
    "apollo-server-express": "2.2.6",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "cloudinary": "1.13.2",
    "compression": "1.7.3",
    "connect-redis": "3.4.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-flash": "0.0.2",
    "express-history-api-fallback": "2.2.1",
    "express-session": "1.15.6",
    "formidable": "1.2.1",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "helmet": "3.15.0",
    "joi": "14.3.0",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "merge-graphql-schemas": "1.5.8",
    "mongoose": "5.3.14",
    "morgan": "1.9.1",
    "node-schedule": "1.3.1",
    "nodemailer": "4.7.0",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
